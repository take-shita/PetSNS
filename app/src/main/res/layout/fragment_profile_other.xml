<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.profile.profile_otherFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/white"
        tools:ignore="UselessParent">
        <!--        ヘッダー-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:orientation="horizontal"
            android:background="@color/white">
            <Space
                android:layout_width="491dp"
                android:layout_height="80dp" />

            <ImageButton
                android:id="@+id/toukoubtn"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:scaleType="fitXY"
                android:src="@drawable/toukoutouka"
                tools:ignore="ContentDescription,SpeakableTextPresentCheck,VisualLintBounds"
                android:background="@android:color/transparent"/>
        </LinearLayout>
        <!--ヘッダー下から-->
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <GridLayout
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:columnCount="4"
                    android:rowCount="2">

                    <Space
                        android:layout_width="35dp"
                        android:layout_height="120dp"
                        android:layout_row="0"
                        android:layout_rowSpan="2"
                        android:layout_column="0" />

                    <TextView
                        android:layout_width="421dp"
                        android:layout_height="90dp"
                        android:layout_row="0"
                        android:layout_column="1"
                        android:gravity="left|bottom"
                        android:text="田實　優斗"
                        android:textSize="35dp"
                        tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="120dp"
                        android:layout_row="0"
                        android:layout_rowSpan="2"
                        android:layout_column="2"
                        android:scaleType="fitXY"
                        android:src="@drawable/cbr250rr"
                        tools:ignore="ContentDescription" />

                    <Space
                        android:layout_width="30dp"
                        android:layout_height="120dp"
                        android:layout_row="0"
                        android:layout_rowSpan="2"
                        android:layout_column="3" />

                    <TextView
                        android:layout_width="421dp"
                        android:layout_height="30dp"
                        android:layout_row="1"
                        android:layout_column="1"
                        android:gravity="left|center"
                        android:text="yuto_1234"
                        android:textSize="22dp"
                        tools:ignore="HardcodedText,RtlHardcoded,SpUsage,TextSizeCheck" />
                </GridLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal"
                    tools:ignore="ExtraText">

                    <Space
                        android:layout_width="30dp"
                        android:layout_height="60dp" />

                    <ToggleButton
                        android:id="@+id/followbtn"
                        android:layout_width="240dp"
                        android:layout_height="30dp"
                        android:textOff=""
                        android:textOn=""
                        android:scaleType="fitCenter"
                        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal">
                    <Space
                        android:layout_width="30dp"
                        android:layout_height="60dp" />

                    <TextView
                        android:layout_width="541dp"
                        android:layout_height="60dp"
                        android:gravity="center"
                        android:text="投稿"
                        android:textSize="24dp"
                        tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:orientation="horizontal">
                    <Space
                        android:layout_width="30dp"
                        android:layout_height="1dp" />
                    <TextView
                        android:layout_width="541dp"
                        android:layout_height="2dp"
                        android:background="@color/black" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/cbr250rr"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:layout_width="210dp"
                            android:layout_height="60dp"
                            android:gravity="left|center"
                            android:text="田實　優斗"
                            android:textSize="24dp"
                            tools:ignore="HardcodedText,RtlHardcoded,SpUsage,TextSizeCheck" />

                        <TextView
                            android:layout_width="120dp"
                            android:layout_height="60dp"
                            android:gravity="center"
                            android:text="17h"
                            android:textSize="24dp"
                            tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />

                        <ImageButton
                            android:id="@+id/hartbtn"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:background="@android:color/transparent"                            android:scaleType="fitXY"
                            android:src="@drawable/harttouka"
                            tools:ignore="ContentDescription,SpeakableTextPresentCheck,ImageContrastCheck" />

                        <TextView
                            android:layout_width="90dp"
                            android:layout_height="60dp"
                            android:gravity="center"
                            android:text="3"
                            android:textSize="24dp"
                            tools:ignore="HardcodedText,SpUsage,TextSizeCheck" />
                        <ImageButton
                            android:id="@+id/reportbtn"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:scaleType="centerInside"
                            android:src="@drawable/report_touka"
                            tools:ignore="ContentDescription,SpeakableTextPresentCheck"
                            android:background="@android:color/transparent"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="left|center"
                            android:text="かわいいねぇ"
                            android:textSize="24dp"
                            tools:ignore="HardcodedText,RtlHardcoded,SpUsage,TextSizeCheck" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="left|center"
                            android:text="#ハリネズミ"
                            android:textColor="#2385DB"
                            android:textSize="24dp"
                            tools:ignore="HardcodedText,RtlHardcoded,SpUsage,TextSizeCheck" />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="#CCCCCC"/>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:scaleType="fitXY"
                            android:src="@drawable/harinezumi"
                            tools:ignore="ContentDescription,SpeakableTextPresentCheck,ImageContrastCheck" />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="#CCCCCC"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/cbr250rr"
                            tools:ignore="ContentDescription" />
                        <TextView
                            android:layout_width="210dp"
                            android:layout_height="60dp"
                            android:gravity="left|center"
                            android:text="田實　優斗"
                            android:textSize="24dp"
                            tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />
                        <TextView
                            android:layout_width="120dp"
                            android:layout_height="60dp"
                            android:gravity="center"
                            android:text="17h"
                            android:textSize="24dp"
                            tools:ignore="HardcodedText,SpUsage" />
                        <ImageButton
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/harttouka"
                            tools:ignore="ContentDescription,SpeakableTextPresentCheck"
                            android:background="@android:color/transparent"/>
                        <TextView
                            android:layout_width="90dp"
                            android:layout_height="60dp"
                            android:gravity="center"
                            android:text="3"
                            android:textSize="24dp"
                            tools:ignore="HardcodedText,SpUsage" />
                        <ImageButton
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/sakujotouka"
                            tools:ignore="ContentDescription,SpeakableTextPresentCheck"
                            android:background="@android:color/transparent"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="left|center"
                            android:text="かわいいねぇ"
                            android:textSize="24dp"
                            tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="left|center"
                            android:text="#ハリネズミ"
                            android:textColor="#2385DB"
                            android:textSize="24dp"
                            tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="#CCCCCC"/>
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleType="fitXY"
                            android:src="@drawable/harinezumi"
                            tools:ignore="ContentDescription,SpeakableTextPresentCheck"
                            android:adjustViewBounds="true"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="2dp"
                            android:background="#CCCCCC"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</FrameLayout>