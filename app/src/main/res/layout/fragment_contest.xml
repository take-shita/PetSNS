<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.contest.ContestFragment">


    <Button
        android:id="@+id/btnContestPost"
        android:layout_width="140dp"
        android:layout_height="55dp"
        android:layout_marginStart="328dp"
        android:layout_marginTop="492dp"
        android:text="投稿"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:onClick="btnContestPostClicked"
        android:enabled="false"/>
    <Button
        android:id="@+id/btnContestEntry"
        android:layout_width="140dp"
        android:layout_height="55dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="492dp"
        android:text="エントリー"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck"
        android:onClick="btnContestEntryClicked"/>

    <Button
        android:id="@+id/btnContestView"
        android:layout_width="140dp"
        android:layout_height="55dp"
        android:layout_marginStart="328dp"
        android:layout_marginTop="564dp"
        android:text="投稿を見る"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <Button
        android:id="@+id/btnContestInfo"
        android:layout_width="140dp"
        android:layout_height="55dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="564dp"
        android:text="詳細情報"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="273dp"
        android:layout_height="238dp"
        android:layout_marginTop="132dp"
        android:src="@mipmap/ic_launcher_round"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="124dp"
        android:layout_height="63dp"
        android:layout_marginStart="388dp"
        android:layout_marginTop="414dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>