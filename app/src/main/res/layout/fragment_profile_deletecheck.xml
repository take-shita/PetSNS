<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="578dp"
android:layout_height="248dp"
tools:context=".ui.profile.profile_deletecheckFragment">
<LinearLayout
    android:layout_width="578dp"
    android:layout_height="248dp"
    android:orientation="vertical">
    <GridLayout
        android:layout_width="578dp"
        android:layout_height="248dp"
        android:columnCount="11"
        android:rowCount="6">
        <TextView
            android:layout_width="578dp"
            android:layout_height="4dp"
            android:layout_row="0"
            android:layout_column="0"
            android:background="#ff0000"
            android:layout_columnSpan="11"/>
        <TextView
            android:layout_width="578dp"
            android:layout_height="4dp"
            android:layout_row="5"
            android:layout_column="0"
            android:background="#ff0000"
            android:layout_columnSpan="11"/>
        <TextView
            android:layout_width="4dp"
            android:layout_height="240dp"
            android:layout_row="1"
            android:layout_column="0"
            android:background="#ff0000"
            android:layout_rowSpan="4"/>
        <TextView
            android:layout_width="4dp"
            android:layout_height="240dp"
            android:layout_row="1"
            android:layout_column="10"
            android:background="#ff0000"
            android:layout_rowSpan="4"/>

        <Space
            android:layout_width="570dp"
            android:layout_height="60dp"
            android:layout_row="1"
            android:layout_column="1"
            android:layout_columnSpan="9"/>
        <Space
            android:layout_width="30dp"
            android:layout_height="120dp"
            android:layout_row="2"
            android:layout_column="1"
            android:layout_rowSpan="2"/>
        <ImageView
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_row="2"
            android:layout_column="2"
            android:layout_rowSpan="2"
            android:scaleType="fitXY"
            android:src="@drawable/bankentouka" />

        <Space
            android:layout_width="30dp"
            android:layout_height="120dp"
            android:layout_row="2"
            android:layout_column="3"
            android:layout_rowSpan="2"/>
        <TextView
            android:layout_width="330dp"
            android:layout_height="60dp"
            android:layout_row="2"
            android:layout_column="4"
            android:gravity="center"
            android:layout_columnSpan="5"
            android:text="投稿を削除しますか？"
            android:textSize="32dp" />
        <Space
            android:layout_width="30dp"
            android:layout_height="60dp"
            android:layout_row="3"
            android:layout_column="4" />
        <ImageButton
            android:id="@+id/haibtn"
            android:layout_width="120dp"
            android:layout_height="60dp"
            android:layout_column="5"
            android:layout_row="3"
            android:scaleType="centerInside"
            android:src="@drawable/hai"
            android:background="@null"
            tools:ignore="SpeakableTextPresentCheck"/>
        <Space
            android:layout_width="30dp"
            android:layout_height="60dp"
            android:layout_row="3"
            android:layout_column="6" />
        <ImageButton
            android:id="@+id/iiebtn"
            android:layout_width="120dp"
            android:layout_height="60dp"
            android:layout_column="7"
            android:layout_row="3"
            android:scaleType="centerInside"
            android:src="@drawable/iie"
            android:background="@null"
            tools:ignore="SpeakableTextPresentCheck"/>
        <Space
            android:layout_width="30dp"
            android:layout_height="60dp"
            android:layout_row="3"
            android:layout_column="8" />
        <Space
            android:layout_width="30dp"
            android:layout_height="120dp"
            android:layout_row="2"
            android:layout_column="9"
            android:layout_rowSpan="2"/>
        <Space
            android:layout_width="570dp"
            android:layout_height="60dp"
            android:layout_row="4"
            android:layout_column="1"
            android:layout_columnSpan="9"/>
    </GridLayout>
</LinearLayout>

</FrameLayout>